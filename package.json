{
  "name": "@enterprise-oss/osso",
  "license": "SEE LICENSE IN LICENSE",
  "version": "0.0.5-alpha.29",
  "description": "React components for Osso",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "sbauch",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "yalc-dev": "yalc-watch",
    "develop": "tsc && rollup -c rollup-dev.config.js --watch",
    "lint": "eslint ./src",
    "format": "tsc && eslint ./src --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.4",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-root-import": "^6.5.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rollup": "^2.18.1",
    "rollup-plugin-root-import": "^1.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "yalc-watch": "^0.0.6"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.10",
    "fast-xml-parser": "^3.17.4",
    "graphql": "^15.1.0"
  },
  "peerDependencies": {
    "fast-xml-parser": "^3.17.4",
    "react": "^16.13.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(png|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/examples"
    ]
  },
  "yalcWatch": {
    "watchFolder": "dist",
    "buildWatchCommand": "tsc && rollup -c rollup-dev.config.js --watch",
    "extensions": "js,png,svg,gif,jpeg,css,map"
  },
  "files": [
    "dist"
  ]
}